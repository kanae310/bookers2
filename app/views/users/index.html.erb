<div class="container">
  <div class="row">

  <div class="col-md-3 ">
  <%= render 'users/list', user: @user %>
  <%= render 'books/list', book: @book %>
  </div>
  <div class="col-md-8 offset-md-1">
   <h1>Users</h1>
   <table class="table table-sm user-index">
    <thead>
    <th>image</th>
    <th>name</th>
    <th></th>
     </thead>
     <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :profile_image, :fill, 10,10,fallback:"no_image.jpg"%></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show","/users/#{user.id}" %></td>
    </tr>
    <%end%>
      </tbody>
     </table>
  </div>

  </div>
</div>
