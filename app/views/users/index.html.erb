<div clas="container">
  <div class="row">
<div class="col-md-3">
<%= render 'users/list', user: @user %>
<%= render 'books/list', book: @book %>
</div>
<div class="col-md-9">
<h1>Users</h1>
<table class="table table-striped">
  <thead>
    <th>image</th>
    <th>name</th>
    <th></th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :profile_image, :fill, 10,10,fallback:"no_image.jpg"%></td>
      <td><%= user.name %></td>
      <td><%= link_to "show", user_path(user.id) %></td>
    </tr>
    <%end%>
  </tbody>
</table>
</div>
</div>
</div>
