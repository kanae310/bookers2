<div class="container">
    <div class="row">
        <div class="col-md-3">
<%= render 'users/list', user:@user %>
<%= render 'books/list', book:@book %>
</div>
<div class="col-md-9">
<h1>Books</h1>
<table class="table table-striped">
    <thead>
        <tr>
        <th>Title</th>
        <th>Body</th>

        </tr>
    </thead>

    <tbody>
        <% @books.each do |book| %>
        <tr>
            <td><%=book.title %></td>
            <td><%=book.body %></td>


        </tr>
        <% end%>
    </tbody>
</table>
</div>
</div>
</div>
